obj-$(CONFIG_PAINTBOX)				+= paintbox-debug.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-dma.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-dma-common.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-dma-debug.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-dma-dram.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-dma-lbp.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-dma-stp.o
obj-$(CONFIG_PAINTBOX_FPGA_SUPPORT)		+= paintbox-fpga.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-io.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-irq.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-lbp.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-lbp-debug.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-mmu.o
obj-$(CONFIG_PAINTBOX_PLATFORM_BUS)		+= paintbox-platform.o
