obj-$(CONFIG_PAINTBOX)				+= paintbox-power.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-sram.o
obj-$(CONFIG_PAINTBOX)				+= paintbox-stp.o
obj-$(CONFIG_PAINTBOX_DEBUG)			+= paintbox-stp-debug.o
