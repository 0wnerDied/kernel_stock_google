 #include <dt-bindings/thermal/thermal.h>

&soc {
	thermal-zones {
		airbrush-tmu {
			/*
			 * TMU is able to emit interrupt to update the thermal
			 * zone while temperature going across the trip points.
			 * Also, each write to emul_temp will always update the
			 * thermal zone. So we set the polling-delay-* to 0.
			 */
			polling-delay-passive = <0>;
			polling-delay = <0>;
			thermal-sensors = <&airbrush_tmu 0>;

			trips {
			/include/ "sm8150-floral-airbrush-dummy-trips.dtsi"
				airbrush_tmu_trip4: trip4 {
					temperature = <65000>;
					hysteresis = <1000>;
					type = "passive";
				};
				airbrush_tmu_trip5: trip5 {
					temperature = <75000>;
					hysteresis = <1000>;
					type = "passive";
				};
				airbrush_tmu_trip6: trip6 {
					temperature = <85000>;
					hysteresis = <1000>;
					type = "passive";
				};
				airbrush_tmu_trip7: trip7 {
					temperature = <90000>;
					hysteresis = <1000>;
					type = "hot";
				};
			};
			cooling-maps {
				map4 {
					trip = <&airbrush_tmu_trip4>;
					cooling-device = <&abc_cooling 1 1>;
				};
				map5 {
					trip = <&airbrush_tmu_trip5>;
					cooling-device = <&abc_cooling 2 2>;
				};
				map6 {
					trip = <&airbrush_tmu_trip6>;
					cooling-device = <&abc_cooling 3 3>;
				};
			};
		};

		airbrush-ipu0 {
			polling-delay-passive = <0>;
